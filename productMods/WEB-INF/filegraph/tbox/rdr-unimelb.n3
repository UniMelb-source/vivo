@prefix ands: <http://purl.org/ands/ontologies/vivo/> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix unimelb-rdr: <https://rdr.unimelb.edu.au/config/> .
@prefix vitro: <http://vitro.mannlib.cornell.edu/ns/vitro/0.7#> .
@prefix vivo: <http://vivoweb.org/ontology/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://rdr.unimelb.edu.au/config/> a owl:Ontology ;
    rdfs:label "Unimelb RDR"@en ;
    dc:contributor "University of Melbourne" ;
    dc:date "2012-06-20" .

# custom classes
unimelb-rdr:ProjectRole a owl:Class;
    rdfs:label "Project Role" ;
    rdfs:subClassOf owl:Thing, vivo:Role, unimelb-rdr:ProjectRole ;
    vitro:descriptionAnnot "Role of a researcher in a project"^^xsd:string ;
    vitro:shortDef "Role of a researcher in a project"^^xsd:string ;
    vitro:displayLimitAnnot "-1"^^xsd:int ;
    vitro:displayRankAnnot "-1"^^xsd:int ;
    vitro:hiddenFromDisplayBelowRoleLevelAnnot <http://vitro.mannlib.cornell.edu/ns/vitro/role#public> ;
    vitro:prohibitedFromUpdateBelowRoleLevelAnnot <http://vitro.mannlib.cornell.edu/ns/vitro/role#public> .

# add a ResearchData to Agreement
unimelb-rdr:agreementHasResearchDataOutput a owl:ObjectProperty ;
    rdfs:label "has research data output"@en ;
    rdfs:domain vivo:Agreement ;
    rdfs:range ands:ResearchData ;
    owl:inverseOf unimelb-rdr:isResearchDataForAgreement ;
    vitro:customEntryFormAnnot "edu.cornell.mannlib.vitro.webapp.edit.n3editing.configuration.generators.AddResearchDataToAgreementGenerator"^^xsd:string ;
    vitro:inPropertyGroupAnnot unimelb-rdr:propertyGroupResearchData ;
    vitro:hiddenFromDisplayBelowRoleLevelAnnot <http://vitro.mannlib.cornell.edu/ns/vitro/role#public> ;
    vitro:prohibitedFromUpdateBelowRoleLevelAnnot <http://vitro.mannlib.cornell.edu/ns/vitro/role#public> ;
    vitro:offerCreateNewOptionAnnot "true"^^xsd:boolean  .
unimelb-rdr:isResearchDataForAgreement a owl:ObjectProperty ;
    rdfs:label "is research data output for agreement"@en ;
    rdfs:domain ands:ResearchData ;
    rdfs:range vivo:Agreement ;
    owl:inverseOf unimelb-rdr:agreementHasResearchDataOutput ;
    vitro:inPropertyGroupAnnot unimelb-rdr:propertyGroupAgreement ;
    vitro:hiddenFromDisplayBelowRoleLevelAnnot <http://vitro.mannlib.cornell.edu/ns/vitro/role#public> ;
    vitro:prohibitedFromUpdateBelowRoleLevelAnnot <http://vitro.mannlib.cornell.edu/ns/vitro/role#public> .

# add a ResearchData to Project
unimelb-rdr:projectHasResearchDataOutput a owl:ObjectProperty ;
    rdfs:label "has research data output"@en ;
    rdfs:domain vivo:Project ;
    rdfs:range ands:ResearchData ;
    owl:inverseOf unimelb-rdr:isResearchDataForProject ;
    vitro:customEntryFormAnnot "edu.cornell.mannlib.vitro.webapp.edit.n3editing.configuration.generators.AddResearchDataToProjectGenerator"^^xsd:string ;
    vitro:inPropertyGroupAnnot unimelb-rdr:propertyGroupResearchData ;
    vitro:hiddenFromDisplayBelowRoleLevelAnnot <http://vitro.mannlib.cornell.edu/ns/vitro/role#public> ;
    vitro:prohibitedFromUpdateBelowRoleLevelAnnot <http://vitro.mannlib.cornell.edu/ns/vitro/role#public> ;
    vitro:offerCreateNewOptionAnnot "true"^^xsd:boolean .
unimelb-rdr:isResearchDataForProject a owl:ObjectProperty ;
    rdfs:label "project"@en ;
    rdfs:domain ands:ResearchData ;
    rdfs:range vivo:Project ;
    owl:inverseOf unimelb-rdr:projectHasResearchDataOutput ;
    vitro:inPropertyGroupAnnot unimelb-rdr:propertyGroupProject ;
    vitro:hiddenFromDisplayBelowRoleLevelAnnot <http://vitro.mannlib.cornell.edu/ns/vitro/role#public> ;
    vitro:prohibitedFromUpdateBelowRoleLevelAnnot <http://vitro.mannlib.cornell.edu/ns/vitro/role#public> .

# add a Project to an Agreement
unimelb-rdr:fundingVehicleFor a owl:ObjectProperty ;
    rdfs:domain vivo:Agreement ;
    rdfs:label "provides funding for"@en-US ;
    vitro:hiddenFromDisplayBelowRoleLevelAnnot <http://vitro.mannlib.cornell.edu/ns/vitro/role#public> ;
    vitro:inPropertyGroupAnnot <http://vivoweb.org/ontology#vitroPropertyGroupresearch> ;
    vitro:offerCreateNewOptionAnnot "true"^^xsd:boolean ;
    vitro:prohibitedFromUpdateBelowRoleLevelAnnot <http://vitro.mannlib.cornell.edu/ns/vitro/role#public> ;
    vitro:selectFromExistingAnnot "true"^^xsd:boolean ;
    owl:inverseOf unimelb-rdr:hasFundingVehicle ;
    vitro:customEntryFormAnnot "edu.cornell.mannlib.vitro.webapp.edit.n3editing.configuration.generators.AddProjectToAgreementGenerator"^^xsd:string .
unimelb-rdr:hasFundingVehicle a owl:ObjectProperty ;
    rdfs:label "funding provided via"@en-US ;
    rdfs:range vivo:Agreement ;
    vitro:hiddenFromDisplayBelowRoleLevelAnnot <http://vitro.mannlib.cornell.edu/ns/vitro/role#public> ;
    vitro:prohibitedFromUpdateBelowRoleLevelAnnot <http://vitro.mannlib.cornell.edu/ns/vitro/role#public> ;
    vitro:selectFromExistingAnnot "true"^^xsd:boolean ;
    owl:inverseOf unimelb-rdr:fundingVehicleFor ;
    vitro:inPropertyGroupAnnot unimelb-rdr:propertyGroupAgreement .

# add a Project to a Person
unimelb-rdr:hasProjectRole a owl:ObjectProperty ;
    rdfs:label "has project role"@en-US ;
    rdfs:domain foaf:Person ;
    rdfs:range unimelb-rdr:ProjectRole ;
    rdfs:subPropertyOf vivo:hasRole , vivo:hasProjectRole , owl:topObjectProperty ;
    vitro:customEntryFormAnnot "edu.cornell.mannlib.vitro.webapp.edit.n3editing.configuration.generators.AddProjectRoleToPersonGenerator"^^xsd:string ;
    vitro:hiddenFromDisplayBelowRoleLevelAnnot <http://vitro.mannlib.cornell.edu/ns/vitro/role#public> ;
    vitro:prohibitedFromUpdateBelowRoleLevelAnnot <http://vitro.mannlib.cornell.edu/ns/vitro/role#public> ;
    vitro:publicDescriptionAnnot "This relates a person to a description of their project."^^xsd:string ;
    vitro:offerCreateNewOptionAnnot "true"^^xsd:boolean ;
    vitro:selectFromExistingAnnot "true"^^xsd:boolean ;
    owl:inverseOf unimelb-rdr:fundingVehicleFor ;
    vitro:inPropertyGroupAnnot unimelb-rdr:propertyGroupProject ;
    owl:inverseof unimelb-rdr:projectRoleOf .
unimelb-rdr:projectRoleOf a owl:ObjectProperty ;
    rdfs:label "project role of"@en-US ;
    rdfs:domain unimelb-rdr:ProjectRole ;
    rdfs:range foaf:Person ;
    rdfs:subPropertyOf vivo:roleOf ;
    owl:inverseOf unimelb-rdr:hasProjectRole .

unimelb-rdr:projectRoleIn a owl:ObjectProperty ;
    rdfs:label "project role in"@en-US ;
    rdfs:domain unimelb-rdr:ProjectRole ;
    rdfs:range vivo:Project ;
    rdfs:subPropertyOf vivo:roleIn, unimelb-rdr:projectRoleIn ;
    owl:inverseOf unimelb-rdr:relatedProjectRole .
unimelb-rdr:relatedProjectRole a owl:ObjectProperty ;
    rdfs:label "related project role"@en-US ;
    rdfs:domain vivo:Project ;
    rdfs:range unimelb-rdr:ProjectRole ;
    rdfs:subPropertyOf vivo:relatedRole, unimelb-rdr:relatedProjectRole ;
    owl:inverseOf unimelb-rdr:projectRoleIn ;
    vitro:inPropertyGroupAnnot unimelb-rdr:propertyGroupResearcher .

unimelb-rdr:collectedDateRange a owl:ObjectProperty ;
    rdfs:label "collected between"@en-US ;
    rdfs:domain ands:ResearchData ;
    rdfs:range vivo:DateTimeInterval ;
    owl:inverseOf unimelb-rdr:collectedDateRangeFor ;
    vitro:customEntryFormAnnot "edu.cornell.mannlib.vitro.webapp.edit.n3editing.configuration.generators.DateTimeIntervalFormGenerator"^^xsd:string .
unimelb-rdr:collectedDateRangeFor a owl:ObjectProperty ;
    rdfs:label "collected range for"@en-US ;
    rdfs:domain vivo:DateTimeInterval ;
    rdfs:range ands:ResearchData ;
    owl:inverseOf unimelb-rdr:collectedDateRange .
    
unimelb-rdr:coveredDateRange a owl:ObjectProperty ;
    rdfs:label "covered between"@en-US ;
    rdfs:domain ands:ResearchData ;
    rdfs:range vivo:DateTimeInterval ;
    owl:inverseOf unimelb-rdr:coveredDateRangeFor ;
    vitro:customEntryFormAnnot "edu.cornell.mannlib.vitro.webapp.edit.n3editing.configuration.generators.DateTimeIntervalFormGenerator"^^xsd:string .
unimelb-rdr:coveredDateRangeFor a owl:ObjectProperty ;
    rdfs:label "covered range for"@en-US ;
    rdfs:domain vivo:DateTimeInterval ;
    rdfs:range ands:ResearchData ;
    owl:inverseOf unimelb-rdr:coveredDateRange .

# inverse relationships for foaf:Person to ands:ResearchData
unimelb-rdr:principalInvestigatorFor a owl:ObjectProperty ;
    rdfs:comment "Principal investigator for"^^xsd:string ;
    rdfs:domain foaf:Person ;
    rdfs:range ands:ResearchData ;
    rdfs:label "principal investigator for"@en ;
